{
	"$schema": "https://typedoc.org/schema.json",
	"name": "Donation Alerts",
	"out": "docs",
	"exclude": [
		"./node_modules",
		"**/node_modules/**",
		"**/*.spec.ts",
		"**/es/**",
		"**/cjs/**",
		"packages/**/cjs/*",
		"packages/**/es/*"
	],
	"entryPoints": ["packages/*"],
	"entryPointStrategy": "packages",
	"cleanOutputDir": true,
	"sort": ["source-order"],
	"includeVersion": true
}
