{
	"name": "Donation Alerts",
	"out": "docs",
	"exclude": [
		"./node_modules",
		"**/node_modules/**",
		"**/*.spec.ts",
		"**/es/**",
		"**/lib/**",
		"packages/**/lib/*",
		"packages/**/es/*"
	],
	"entryPoints": ["packages/*"],
	"entryPointStrategy": "packages",
	"cleanOutputDir": true,
	"sort": ["source-order"],
	"includeVersion": true
}
