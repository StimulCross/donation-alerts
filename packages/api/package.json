{
	"name": "@donation-alerts/api",
	"version": "4.0.0-next.2",
	"publishConfig": {
		"access": "public"
	},
	"author": "StimulCross <stimulcross@gmail.com>",
	"description": "Interact with Donation Alerts API.",
	"homepage": "https://github.com/StimulCross/donation-alerts/tree/main/packages/api#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StimulCross/donation-alerts.git",
		"directory": "packages/api"
	},
	"engines": {
		"node": ">=20"
	},
	"keywords": [
		"donation-alerts",
		"api",
		"donations",
		"tips"
	],
	"license": "MIT",
	"main": "cjs",
	"module": "es",
	"types": "cjs",
	"exports": {
		".": {
			"types": "./cjs/index.d.ts",
			"require": "./cjs/index.js",
			"import": "./es/index.mjs",
			"default": "./cjs/index.js"
		}
	},
	"scripts": {
		"build": "tsukuru -c tsconfig.build.json",
		"rebuild": "tsukuru --clean -c tsconfig.build.json",
		"clean": "rimraf es && rimraf cjs"
	},
	"files": [
		"cjs",
		"es",
		"LICENSE",
		"README.md",
		"!cjs/**/*.d.ts.map",
		"!es/**/*.d.ts.map"
	],
	"dependencies": {
		"@d-fischer/rate-limiter": "^1.0.1",
		"@donation-alerts/api-call": "^4.0.0-next.2",
		"@donation-alerts/common": "^4.0.0-next.2",
		"@stimulcross/logger": "^7.0.0",
		"@stimulcross/shared-utils": "^0.2.3",
		"cross-sha256": "^1.2.0",
		"qs": "^6.14.0",
		"typescript-memoize": "^1.1.1"
	},
	"peerDependencies": {
		"@donation-alerts/auth": "3.0.1"
	},
	"devDependencies": {
		"@donation-alerts/auth": "^4.0.0-next.2",
		"@types/qs": "^6.9.18",
		"rimraf": "^6.0.1",
		"tslib": "^2.8.1",
		"tsukuru": "^0.8.3"
	}
}
